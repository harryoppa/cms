{
    "name": "tvhung/platform",
    "description": "TVHung Platform based on Laravel Framework.",
    "keywords": [
        "tvhung",
        "tvhung platform"
    ],
    "homepage": "https://tranvinhhung.com",
    "authors": [
        {
            "name": "Tran Vinh Hung",
            "email": "me@tranvinhhung.com"
        }
    ],
    "type": "project",
    "require": {
        "barryvdh/laravel-dompdf": "^0.9.0",
        "botble/assets": "^1.0",
        "intervention/image": "^2.4",
        "kris/laravel-form-builder": "1.16.x-dev",
        "laravelcollective/html": "^6.1",
        "xantios/mimey": "^2.1",
        "tightenco/ziggy": "^1.0",
        "yajra/laravel-datatables-buttons": "^4.9",
        "yajra/laravel-datatables-oracle": "^9.9",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "mews/purifier": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "TVHung\\Support\\": "support/src",
            "TVHung\\ACL\\": "acl/src",
            "TVHung\\Base\\": "base/src",
            "TVHung\\Dashboard\\": "dashboard/src",
            "TVHung\\Media\\": "media/src",
            "TVHung\\Setting\\": "setting/src",
            "TVHung\\Table\\": "table/src",
            "TVHung\\JsValidation\\": "js-validation/src",
            "TVHung\\Chart\\": "chart/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TVHung\\Base\\Providers\\BaseServiceProvider",
                "TVHung\\Base\\Providers\\CommandServiceProvider",
                "TVHung\\Base\\Providers\\EventServiceProvider",
                "TVHung\\Base\\Providers\\BreadcrumbsServiceProvider",
                "TVHung\\Base\\Providers\\ComposerServiceProvider",
                "TVHung\\Base\\Providers\\MailConfigServiceProvider",
                "TVHung\\Base\\Providers\\FormServiceProvider",
                "TVHung\\Support\\Providers\\SupportServiceProvider",
                "TVHung\\Table\\Providers\\TableServiceProvider",
                "TVHung\\ACL\\Providers\\AclServiceProvider",
                "TVHung\\Dashboard\\Providers\\DashboardServiceProvider",
                "TVHung\\Media\\Providers\\MediaServiceProvider",
                "TVHung\\JsValidation\\Providers\\JsValidationServiceProvider",
                "TVHung\\Chart\\Providers\\ChartServiceProvider"
            ],
            "aliases": {
                "Assets": "TVHung\\Base\\Facades\\AssetsFacade",
                "BaseHelper": "TVHung\\Base\\Facades\\BaseHelperFacade",
                "MetaBox": "TVHung\\Base\\Facades\\MetaBoxFacade",
                "Action": "TVHung\\Base\\Facades\\ActionFacade",
                "Filter": "TVHung\\Base\\Facades\\FilterFacade",
                "EmailHandler": "TVHung\\Base\\Facades\\EmailHandlerFacade",
                "Breadcrumbs": "TVHung\\Base\\Facades\\BreadcrumbsFacade",
                "JsValidator": "TVHung\\JsValidation\\Facades\\JsValidatorFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
