{
    "name": "tvhung/widget",
    "description": "Widget package",
    "type": "package",
    "autoload": {
        "psr-4": {
            "TVHung\\Widget\\": "src"
        }
    },
    "require": {
        "tvhung/platform": "*@dev",
        "tvhung/theme": "*@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TVHung\\Widget\\Providers\\WidgetServiceProvider"
            ],
            "aliases": {
                "Widget": "TVHung\\Widget\\Facades\\WidgetFacade",
                "WidgetGroup": "TVHung\\Widget\\Facades\\WidgetGroupFacade"
            }
        }
    }
}
